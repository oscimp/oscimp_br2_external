 						&spec_fpga->dbg_csr_reg);
@@ -150,6 +155,7 @@ static int spec_fpga_dbg_init(struct spec_fpga *spec_fpga)
 			SPEC_DBG_CSR_NAME, err);
 		goto err;
 	}
+#endif
 
 	spec_fpga->dbg_bld = debugfs_create_file(SPEC_DBG_BLD_INFO_NAME,
 						 0444,
